    "test": "set -e; npm run lint; npm run mocha; npm run jest",
    "jest": "node scripts/prepare-jest; jest --config=jest.config.json",
    "mocha": "node scripts/mocha",
    "lint": "eslint .",
    "foo": "jest --config=jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robert-W/mern-kit.git"
