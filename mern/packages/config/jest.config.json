{
  "verbose": true,
  "collectCoverage": true,
  "coverageReporters": [
    "text"
  ],
  "moduleNameMapper": {
    "\\.(css|scss)$": "<rootDir>/packages/__mocks__/object.js",
    "\\.(jpe?g|png|gif|svg)$": "<rootDir>/packages/__mocks__/object.js",
    "^common(.*)$": "/srv/src/packages/client/common$1",
    "^home(.*)$": "/srv/src/packages/client/home$1",
    "^login(.*)$": "/srv/src/packages/client/login$1"
  },
  "testMatch": [
    "<rootDir>/packages/client/*/tests/**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/lib/",
    "/config/",
    "/env/",
    "/utils/",
    "/models/"
  ]
}